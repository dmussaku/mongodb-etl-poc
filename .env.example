# ETL Service Environment Configuration

# Application Settings
APP_NAME="ETL Service"
DEBUG=true
LOG_LEVEL=INFO

# Database Configuration
DATABASE_URL=postgresql://postgres:password@postgres:5432/etl_db

# Redis Configuration  
REDIS_URL=redis://redis:6379/0

# Celery Configuration
CELERY_BROKER_URL=amqp://admin:password@rabbitmq:5672//
CELERY_RESULT_BACKEND=redis://redis:6379/1

# MongoDB Configuration (Source Database)
MONGODB_URL=mongodb://root:password@mongodb:27017/

# Security
SECRET_KEY=your-super-secret-key-change-in-production

# API Configuration
API_V1_PREFIX=/api/v1